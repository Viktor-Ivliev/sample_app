<p id="notice"><%= notice %></p>

<h1>Listing Status Of Implementations</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Date status</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @status_of_implementations.each do |status_of_implementation| %>
      <tr>
        <td><%= status_of_implementation.order_id %></td>
        <td><%= status_of_implementation.date_status %></td>
        <td><%= status_of_implementation.status %></td>
        <td><%= link_to 'Show', status_of_implementation %></td>
        <td><%= link_to 'Edit', edit_status_of_implementation_path(status_of_implementation) %></td>
        <td><%= link_to 'Destroy', status_of_implementation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Status of implementation', new_status_of_implementation_path %>
