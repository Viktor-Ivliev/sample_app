%p#notice= notice

%h1 Listing Table Service Orders

%table
  %thead
    %tr
      %th Order
      %th Service
      %th Price fact
      %th Info
      %th{ "colspan" => "3" }

  %tbody
    - @table_service_orders.each do |table_service_order|
      %tr
        %td= table_service_order.order_id
        %td= table_service_order.service_id
        %td= table_service_order.price_fact
        %td= table_service_order.info
        %td= link_to 'Show', table_service_order
        %td= link_to 'Edit', edit_table_service_order_path(table_service_order)
        %td= link_to 'Destroy', table_service_order, method: :delete, data: { confirm: 'Are you sure?' }
  
%br

= link_to 'New Table service order', new_table_service_order_path
