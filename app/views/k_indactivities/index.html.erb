<p id="notice"><%= notice %></p>

<h1>Listing K Indactivities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @k_indactivities.each do |k_indactivity| %>
      <tr>
        <td><%= k_indactivity.name %></td>
        <td><%= link_to 'Show', k_indactivity %></td>
        <td><%= link_to 'Edit', edit_k_indactivity_path(k_indactivity) %></td>
        <td><%= link_to 'Destroy', k_indactivity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New K indactivity', new_k_indactivity_path %>
